{
  "name": "new-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rescripts  build",
    "check": "npx eslint src --ext .js,.ts,.tsx && npx stylelint 'src/**/*.*css' && npx prettier --check .",
    "eject": "rescripts  eject",
    "fix": "npx eslint src --ext .js,.ts,.tsx --fix && npx stylelint 'src/**/*.*css' --fix && npx prettier --write .",
    "pr": "npm run fix && npm run test:coverage",
    "start": "rescripts  start",
    "test": "rescripts  test --env=jest-environment-jsdom-sixteen",
    "test:coverage": "rescripts  test --env=jest-environment-jsdom-sixteen --coverage --watchAll=false"
  },
  "rescripts": [
    [
      "use-babel-config",
      {
        "presets": [
          "react-app",
          [
            "@babel/preset-typescript",
            {
              "allowNamespaces": true
            }
          ]
        ]
      }
    ]
  ],
  "dependencies": {
    "@rescripts/rescript-use-babel-config": "0.0.10",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.2.2",
    "@types/classnames": "^2.2.11",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.19.7",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "bootstrap": "^4.5.3",
    "date-fns": "^2.16.1",
    "classnames": "^2.2.6",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "history": "^5.0.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lodash": "^4.17.20",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "prettier-stylelint": "^0.4.2",
    "pretty-format": "^26.4.2",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "typescript": "^4.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.165",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@rescripts/cli": "0.0.15"
  }
}
